      ],
      "exposed" : false
    },
    "submit_errors_list" : {
      "type" : "expression",
      "value" : [
        "if($data.formOutput && $data.formOutput._submitError && $data.formOutput._submitError.explanations){",
        "\tconst liElements = $data.formOutput._submitError.explanations",
        "\t\t.filter(cause => cause !== null)",
        "\t\t.map(cause => \"<li>\" + cause + \"</li>\")",
        "\t\t.join('');",
        "\tif(liElements){",
        "\t\treturn \"<ul>\" + liElements + \"</ul>\";",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
    "formOutput" : {
      "type" : "expression",
      "value" : [
        "if( $data.demande ){\r",
        "\treturn {\r",
        "\t\t//map demande variable to expected task contract input\r",
        "\t\tdemandeInput: {\r",
        "\t\t\tmontantChoisi: $data.demande.montantChoisi,\r",
        "\t\t\tmontantRemboursement: $data.demande.montantRemboursement\r",
        "\t\t}\r",
        "\t};\r",
        "}"
      ],
      "exposed" : false
    },
    "context" : {
      "type" : "url",
      "value" : [
        "../API/bpm/userTask/{{taskId}}/context"
      ],
      "exposed" : false
    },
    "taskId" : {
      "type" : "urlparameter",
      "value" : [
        "id"
      ],
      "exposed" : false
    },
    "demande" : {
      "type" : "url",
      "value" : [
        "../{{context.demande_ref.link}}"
      ],
      "exposed" : false
    },
    "montantRemboursement" : {
      "type" : "expression",
      "value" : [
        "function delayedAction() {",
        "    var buttonSubmit = document.querySelector('.buttonSubmit button');",
        "    var montantChoisi = document.querySelector('.inputMontant input').value;",
        "    var divHidden = document.querySelector('.hiddenMontant input');",
        "    divHidden.disabled = true;",
        "    var div = document.querySelector('.montantRemboursement p');",
        "    var tauxInteret = null;",
        "    ",
        "    if($data.demande.dureeEmprunt === 7){",
        "        tauxInteret = 2.025;",
        "    } else if ($data.demande.dureeEmprunt === 10){",
        "        tauxInteret = 2.1;",
        "    } else if ($data.demande.dureeEmprunt === 15){",
        "        tauxInteret = 2.275;",
        "    } else if ($data.demande.dureeEmprunt === 20){",
        "        tauxInteret = 2.375;",
        "    } else if ($data.demande.dureeEmprunt === 25){",
        "        tauxInteret = 2.45;",
        "    }",
        "    ",
        "    var montant = $data.demande.montantChoisi + ($data.demande.montantChoisi * (tauxInteret/100));",
        "    ",
        "    buttonSubmit.onclick = function() {",
        "        divHidden.value = montant;",
        "    };",
        "    ",
        "    $data.demande.montantRemboursement = montant;",
        "    return montant;",
        "}",
        "",
        "setTimeout(delayedAction, 100);"
      ],
      "exposed" : false
    }
  },
