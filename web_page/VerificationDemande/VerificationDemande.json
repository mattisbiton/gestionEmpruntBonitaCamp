    "submit_errors_list" : {
      "type" : "expression",
      "value" : [
        "if($data.formOutput && $data.formOutput._submitError && $data.formOutput._submitError.explanations){",
        "\tconst liElements = $data.formOutput._submitError.explanations",
        "\t\t.filter(cause => cause !== null)",
        "\t\t.map(cause => \"<li>\" + cause + \"</li>\")",
        "\t\t.join('');",
        "\tif(liElements){",
        "\t\treturn \"<ul>\" + liElements + \"</ul>\";",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
    "formOutput" : {
      "type" : "expression",
      "value" : [
        "if( $data.demande ){\r",
        "\treturn {\r",
        "\t\t//map demande variable to expected task contract input\r",
        "\t\tdemandeInput: {\r",
        "\t\t    capaciteEmprunt : $data.capaciteEmprunt,\r",
        "\t\t\testApprouvee: $data.demande.estApprouvee !== undefined ? $data.demande.estApprouvee : null\r",
        "\t\t}\r",
        "\t}\r",
        "}"
      ],
      "exposed" : false
    },
    "context" : {
      "type" : "url",
      "value" : [
        "../API/bpm/userTask/{{taskId}}/context"
      ],
      "exposed" : false
    },
    "taskId" : {
      "type" : "urlparameter",
      "value" : [
        "id"
      ],
      "exposed" : false
    },
    "demande" : {
      "type" : "url",
      "value" : [
        "../{{context.demande_ref.link}}"
      ],
      "exposed" : false
    },
    "demandeur" : {
      "type" : "url",
      "value" : [
        "../API/identity/user/{{demande.idDemandeur}}"
      ],
      "exposed" : false
    },
    "r" : {
      "type" : "expression",
      "value" : [
        "var sommeRevenus = $data.demande.salaire + $data.demande.autreRevenus;",
        "return sommeRevenus;"
      ],
      "exposed" : false
    },
    "charges" : {
      "type" : "expression",
      "value" : [
        "var sommeCharges = $data.demande.loyer + $data.demande.charge; ",
        "return sommeCharges;"
      ],
      "exposed" : false
    },
    "tauxEndettement" : {
      "type" : "expression",
      "value" : [
        "var taux = ($data.charges*100)/$data.r;",
        "return taux;"
      ],
      "exposed" : false
    },
    "capaciteEmprunt" : {
      "type" : "expression",
      "value" : [
        "var tauxInteret = null;",
        "",
        "if($data.demande.dureeEmprunt === 7) {",
        "    tauxInteret = 2.025;",
        "} else if($data.demande.dureeEmprunt === 10) {",
        "    tauxInteret = 2.1;",
        "} else if($data.demande.dureeEmprunt === 15) {",
        "    tauxInteret = 2.275;",
        "} else if($data.demande.dureeEmprunt === 20) {",
        "    tauxInteret = 2.375;",
        "} else if($data.demande.dureeEmprunt === 25) {",
        "    tauxInteret = 2.45;",
        "}",
        "",
        "var capacite = ($data.r*0.33 - $data.charges)*($data.demande.dureeEmprunt*12);",
        "",
        "return capacite;"
      ],
      "exposed" : false
    }
  },
